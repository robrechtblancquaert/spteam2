<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.LoginController">
   <children>
      <TextField fx:id="loginPass" layoutX="39.0" layoutY="88.0" promptText="Password" />
      <TextField fx:id="loginName" layoutX="39.0" layoutY="56.0" promptText="Username" />
      <Button layoutX="39.0" layoutY="135.0" mnemonicParsing="false" onAction="#changeScreenButtonPushed" text="Login" />
      <Label layoutX="39.0" layoutY="20.0" text="Welcome">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="status" layoutX="14.0" layoutY="178.0" text="Status" />
   </children>
</AnchorPane>
